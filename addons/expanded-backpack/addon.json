{
  "name": "Multiple rows in backpack",
  "description": "Display the contents of the editor backpack in an expandable two-way grid similar to the costume library and increase the size of thumbnails to improve ease of use.",
  "credits": [
    {
      "name": "pumpkinhasapatch",
      "link": "https://github.com/pumpkinhasapatch"
    }
  ],
  "versionAdded": "1.30.0",
  "tags": ["editor"],
  "enabledByDefault": false,
  "settings": [
    {
      "name": "Number of displayed rows",
      "id": "rows",
      "type": "integer",
      "min": 1,
      "max": 4,
      "default": 2
    },
    {
      "name": "Larger costume and script thumbnails",
      "id": "upscale",
      "type": "boolean",
      "default": true
    }
  ],
  "customCssVariables": [
    {
      "name": "imgsize",
      "value": {
        "type": "ternary",
        "source": {
          "type": "settingValue",
          "settingId": "upscale"
        },
        "true": "95%",
        "false": "32px"
      }
    },
    {
      "name": "bgcolor",
      "value": {
        "type": "ternary",
        "source": {
          "type": "settingValue",
          "settingId": "upscale"
        },
        "true": "white",
        "false": "transparent"
      }
    }
  ],
  "userstyles": [
    {
      "url": "backpack.css",
      "matches": ["projects"]
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["projects"]
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true
}
