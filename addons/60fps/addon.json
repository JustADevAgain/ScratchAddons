  "info": [
    {
      "type": "notice",
      "text": "Enabling the custom FPS mode changes the project speed. Projects typically run at 30 FPS.",
      "id": "projectRunsFasterNotice"
    }
  ],
  "credits": [
    {
      "name": "Jeffalo",
      "link": "https://scratch.mit.edu/users/Jeffalo"
    },
    {
      "name": "TheColaber",
      "link": "https://scratch.mit.edu/users/TheColaber"
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/projects/*"]
