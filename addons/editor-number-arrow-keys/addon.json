{
  "name": "Arrow key incrementation in editor",
  "description": "Use the up and down arrow keys to increase or decrease number values in numeric input fields such as those in blocks or the sprite properties area.",
  "tags": ["editor", "codeEditor", "recommended"],
  "versionAdded": "1.34.0",
  "latestUpdate": {
    "version": "1.35.0",
    "newSettings": ["useCustom"],
    "temporaryNotice": "You can now increment input fields that are not in blocks, like the sprite properties, and fully customize the values."
  },
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["projects"]
    }
  ],
  "settings": [
    {
      "name": "Change on regular key press",
      "id": "regular",
      "type": "number",
      "default": 1,
      "min": 0,
      "max": 9999999999.999998,
      "step": "any"
    },
    {
      "name": "Change on Shift+Key",
      "id": "shift",
      "type": "number",
      "default": 10,
      "min": 0,
      "max": 9999999999.999998,
      "step": "any"
    },
    {
      "name": "Change on Alt+Key",
      "id": "alt",
      "type": "number",
      "default": 0.1,
      "min": 0,
      "max": 9999999999.999998,
      "step": "any"
    }
  ],
  "credits": [
