{
  "name": "Custom Navbar",
  "description": "Change and tweak all the buttons on that Navigation bar.",
  "credits": [
    {
      "name": "TheColaber",
      "link": "https://scratch.mit.edu/users/TheColaber"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/*"],
      "runAtComplete": false
    }
  ],
  "settings": [
    {
      "type": "dynamic-length",
      "template": [
        {
          "name": "Item",
          "id": "item",
          "type": "string",
          "default": "Item name"
        },
        {
          "name": "Item url",
          "id": "itemurl",
          "type": "string",
          "default": "Item Url"
        }
      ],
      "defaults": [
        [
          {
            "id": "create",
            "value": "Create"
          },
          "https://scratch.mit.edu/create"
        ],
        [
          {
            "id": "explore",
            "value": "Explore"
          },
          "https://scratch.mit.edu/explore/projects/all"
        ],
        [
          {
            "id": "ideas",
            "value": "Ideas"
          },
          "https://scratch.mit.edu/ideas"
        ],
        [
          {
            "id": "about",
            "value": "About"
          },
          "https://scratch.mit.edu/about"
        ]
      ]
    }
  ],
  "tags": ["community"],
  "l10n": true
}
