{
  "name": "Project player dark mode and customizable colors",
  "description": "Customize colors used by the project player. Multiple dark themes by different authors available. If you don't need dark mode but want to change the default colors you can select the \"Scratch's default colors\" preset and tweak it.",
  "credits": [
    {
      "name": "Maximouse",
      "note": "addon, \"Experimental\" Dark",
      "id": "addon",
      "link": "https://scratch.mit.edu/users/Maximouse/"
    },
    {
      "name": "Samq64",
      "note": "addon",
      "id": "addon2",
      "link": "https://github.com/Samq64"
    },
    {
      "name": "infinitytec",
      "note": "3.Dark, 3.Darker",
      "id": "darker",
      "link": "https://github.com/infinitytec"
    },
    {
      "name": "_nix",
      "note": "Dark editor",
      "id": "dark",
      "link": "https://github.com/towerofnix"
    },
    {
      "name": "GarboMuffin",
      "note": "TurboWarp Dark",
      "id": "twdark",
      "link": "https://github.com/GarboMuffin"
    }
  ],
  "customCssVariables": [
    {
      "name": "page-text",
      "value": {
        "type": "textColor",
        "source": {
          "type": "settingValue",
          "settingId": "page"
        }
      }
    },
    {
      "name": "primary-filter",
      "value": {
        "type": "textColor",
        "black": "brightness(0.4)",
        "white": "none",
        "source": {
          "type": "settingValue",
          "settingId": "primary"
        }
      }
    },
    {
      "name": "primary-transparent35",
      "value": {
        "type": "multiply",
        "source": {
          "type": "settingValue",
          "settingId": "primary"
        },
        "a": 0.35
      }
    },
    {
      "name": "accent-text",
      "value": {
        "type": "textColor",
        "source": {
          "type": "settingValue",
          "settingId": "accent"
        }
      }
    },
    {
      "name": "input-text",
      "value": {
        "type": "textColor",
        "source": {
          "type": "settingValue",
          "settingId": "input"
        }
      }
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "versionAdded": "1.30.0",
  "updateUserstylesOnSettingsChange": true,
  "userscripts": [
    {
      "url": "bubbles.js",
      "matches": ["projects"]
    }
  ],
  "userstyles": [
    {
      "url": "stage.css",
      "matches": ["projects"]
    }
  ],
  "settings": [
    {
      "name": "Page background",
      "id": "page",
      "type": "color",
      "default": "#111111"
    },
    {
      "name": "Highlight color",
      "id": "primary",
      "type": "color",
      "default": "#4d97ff"
    },
    {
      "name": "Accent background",
      "id": "accent",
      "type": "color",
      "default": "#151515"
    },
    {
      "name": "Input background",
      "id": "input",
      "type": "color",
      "default": "#202020"
    },
    {
      "name": "Full screen background",
      "id": "fullscreen",
      "type": "color",
      "default": "#000000"
    },
    {
      "name": "Full screen header background",
      "id": "stageHeader",
      "type": "color",
      "default": "#000000"
    },
    {
      "name": "Border color",
      "id": "border",
      "type": "color",
      "default": "#ffffff0d",
      "allowTransparency": true
    }
  ],
  "presets": [
    {
      "name": "3.Darker",
      "id": "3darker",
      "description": "A dark theme based on a userscript by infinitytec",
      "values": {
        "page": "#111111",
        "primary": "#4d97ff",
        "accent": "#151515",
        "input": "#202020",
        "fullscreen": "#000000",
        "stageHeader": "#000000",
        "border": "#ffffff0d"
      }
    },
    {
      "name": "3.Dark",
      "id": "3dark",
      "description": "A dark blue theme based on a userscript by infinitytec",
      "values": {
        "page": "#051638",
        "primary": "#4d97ff",
        "accent": "#273552",
        "input": "#2f4066",
        "fullscreen": "#051638",
        "stageHeader": "#051638",
        "border": "#00000026"
      }
    },
    {
      "name": "Dark Editor",
