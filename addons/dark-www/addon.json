  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "versionAdded": "1.4.0",
  "updateUserstylesOnSettingsChange": true,
  "userscripts": [
    {
      "url": "experimental_forums_preview.js",
      "matches": ["editingScreens"]
    }
  ],
  "userstyles": [
    {
      "url": "scrollbar.css",
      "matches": ["https://scratch.mit.edu/*"],
      "if": {
        "settings": { "darkScrollbars": true }
      }
    },
    {
      "url": "experimental_scratchr2.css",
      "matches": ["isNotScratchWWW"]
    },
    {
      "url": "experimental_scratchwww.css",
      "matches": ["scratchWWWNoProject", "projects"]
    },
    {
      "url": "experimental_scratchwww_white_bg.css",
      "matches": [
        "https://scratch.mit.edu/ideas",
        "https://scratch.mit.edu/annual-report"
      ]
    },
    {
      "url": "darker_banners.css",
      "matches": ["scratchWWWNoProject"],
      "if": {
        "settings": { "darkBanners": "darker" }
      }
    },
    {
      "url": "desaturated_banners.css",
      "matches": ["scratchWWWNoProject"],
      "if": {
        "settings": { "darkBanners": "desaturated" }
      }
    },
    {
      "url": "experimental_search.css",
      "matches": ["https://scratch.mit.edu/explore/*", "https://scratch.mit.edu/search/*"]
    },
    {
      "url": "explore_desaturated_banner.css",
      "matches": ["https://scratch.mit.edu/explore/*"],
      "if": {
        "settings": { "darkBanners": "desaturated" }
      }
    },
    {
      "url": "search_desaturated_banner.css",
      "matches": ["https://scratch.mit.edu/search/*"],
      "if": {
        "settings": { "darkBanners": "desaturated" }
      }
    },
    {
      "url": "experimental_forums.css",
      "matches": ["https://scratch.mit.edu/discuss/*"]
    },
    {
      "url": "experimental_profile.css",
      "matches": ["https://scratch.mit.edu/users/*", "https://scratch.mit.edu/classes/*"]
    },
    {
      "url": "experimental_scratchr2_comments.css",
      "matches": ["https://scratch.mit.edu/users/*"]
    },
    {
      "url": "experimental_remixtree.css",
      "matches": ["https://scratch.mit.edu/projects/*/remixtree/"]
    },
    {
      "url": "experimental_mystuff.css",
      "matches": ["https://scratch.mit.edu/mystuff/"]
    },
    {
      "url": "experimental_settings.css",
      "matches": [
        "https://scratch.mit.edu/accounts/settings/",
        "https://scratch.mit.edu/accounts/password_change/",
        "https://scratch.mit.edu/accounts/email_change/"
      ]
    },
    {
      "url": "experimental_confirm_email_modal.css",
      "matches": ["https://scratch.mit.edu/accounts/email_resend_standalone/"]
    },
    {
      "url": "experimental_statistics.css",
      "matches": ["https://scratch.mit.edu/statistics/"]
    },
    {
      "url": "experimental_quotes.css",
      "matches": ["https://scratch.mit.edu/info/quotes/"]
    },
    {
      "url": "experimental_project_embed.css",
      "matches": ["projectEmbeds"]
    },
    {
      "url": "pygments.css",
      "matches": ["forums"]
    }
  ],
  "settings": [
    {
      "name": "Page background",
      "id": "page",
      "type": "color",
      "default": "#202020"
    },
    {
      "name": "Navigation bar background",
      "id": "navbar",
      "type": "color",
      "default": "#4d97ff"
    },
    {
      "name": "Content background",
      "id": "box",
      "type": "color",
      "default": "#282828"
    },
    {
      "name": "Gray background",
      "id": "gray",
      "type": "color",
      "default": "#333333"
    },
    {
      "name": "Blue background",
      "id": "blue",
      "type": "color",
      "default": "#030b16"
    },
    {
      "name": "Input background",
      "id": "input",
      "type": "color",
      "default": "#202020"
    },
    {
      "name": "Highlight color",
      "id": "button",
      "type": "color",
      "default": "#4d97ff"
    },
    {
      "name": "Link color",
      "id": "link",
      "type": "color",
      "default": "#4d97ff"
    },
    {
      "name": "Footer background",
      "id": "footer",
      "type": "color",
      "default": "#333333"
    },
    {
      "name": "Border color",
      "id": "border",
      "type": "color",
      "allowTransparency": true,
      "default": "#606060"
    },
    {
      "name": "Colored areas",
      "id": "darkBanners",
      "type": "select",
      "potentialValues": [
        {
          "id": "off",
          "name": "Default"
        },
        {
          "id": "darker",
          "name": "Darker"
        },
        {
          "id": "desaturated",
          "name": "Darker and desaturated"
        }
      ],
      "default": "darker"
    },
    {
      "name": "Dark scrollbars",
      "id": "darkScrollbars",
      "type": "boolean",
      "default": true
    }
  ],
  "presets": [
    {
