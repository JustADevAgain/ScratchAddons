{
  "name": "Website dark mode",
  "description": "Dark theme for the Scratch website.",
  "tags": ["community", "theme", "recommended"],
  "credits": [
    {
      "name": "Maximouse",
      "link": "https://scratch.mit.edu/users/Maximouse/"
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "versionAdded": "1.4.0",
  "userscripts": [
    {
      "url": "experimental_forums_preview.js",
      "matches": [
        "https://scratch.mit.edu/discuss/topic/*",
        "https://scratch.mit.edu/discuss/post/*/edit/",
        "https://scratch.mit.edu/discuss/settings/*/"
      ]
    }
  ],
  "userstyles": [
    {
      "url": "scrollbar.css",
      "matches": ["https://scratch.mit.edu/*"]
    },
    {
      "url": "experimental_scratchr2.css",
      "matches": [
        "https://scratch.mit.edu/discuss/*",
        "https://scratch.mit.edu/mystuff/",
        "https://scratch.mit.edu/users/*",
        "https://scratch.mit.edu/classes/*",
        "https://scratch.mit.edu/projects/*/remixes/",
        "https://scratch.mit.edu/projects/*/studios/",
        "https://scratch.mit.edu/projects/*/remixtree/",
        "https://scratch.mit.edu/accounts/*",
        "https://scratch.mit.edu/statistics/",
        "https://scratch.mit.edu/info/quotes/",
        "https://scratch.mit.edu/info/ext_download/",
        "https://scratch.mit.edu/cloudmonitor/*",
        "https://scratch.mit.edu/403/",
        "https://scratch.mit.edu/404/",
        "https://scratch.mit.edu/500/",
        "https://scratch.mit.edu/503/",
        "https://scratch.mit.edu/login",
        "https://scratch.mit.edu/login_retry",
        "https://scratch.mit.edu/news"
      ]
    },
    {
      "url": "experimental_scratchwww.css",
      "matches": [
        "scratchWWWNoProject",
        "projects"
      ]
    },
    {
      "url": "experimental_forums.css",
      "matches": ["https://scratch.mit.edu/discuss/*"]
    },
    {
      "url": "experimental_profile.css",
      "matches": ["https://scratch.mit.edu/users/*", "https://scratch.mit.edu/classes/*"]
    },
    {
      "url": "experimental_scratchr2_comments.css",
      "matches": ["https://scratch.mit.edu/users/*"]
    },
    {
      "url": "experimental_remixtree.css",
      "matches": ["https://scratch.mit.edu/projects/*/remixtree/"]
    },
    {
      "url": "experimental_mystuff.css",
      "matches": ["https://scratch.mit.edu/mystuff/"]
    },
    {
      "url": "experimental_settings.css",
      "matches": [
        "https://scratch.mit.edu/accounts/settings/",
        "https://scratch.mit.edu/accounts/password_change/",
        "https://scratch.mit.edu/accounts/email_change/"
      ]
    },
    {
      "url": "experimental_confirm_email_modal.css",
      "matches": ["https://scratch.mit.edu/accounts/email_resend_standalone/"]
    },
    {
      "url": "experimental_statistics.css",
      "matches": ["https://scratch.mit.edu/statistics/"]
    },
    {
      "url": "experimental_quotes.css",
      "matches": ["https://scratch.mit.edu/info/quotes/"]
    },
    {
      "url": "experimental_project_embed.css",
      "matches": ["https://scratch.mit.edu/projects/*/embed"]
    },
    {
      "url": "pygments.css",
      "matches": ["https://scratch.mit.edu/discuss/topic/*"]
    }
  ]
}
