{
    "name": "Custom Fonts",
    "description": "Customize the font. Pick any font from https://fonts.google.com/ and type it below.",
    "tags": ["theme", "beta"],
    "credits": [
        {
            "name": "ErrorGamer2000",
            "link": "https://github.com/ErrorGamer2000"
        }
    ],
    "userscripts": [
        {
            "url": "userscript.js",
            "matches": ["https://scratch.mit.edu/*"],
            "runAtComplete": true
        }
    ],
    "settings": [
        {
            "name": "Font Family",
            "id": "fontFamily",
            "type": "string",
            "default": "Roboto"
        },
        {
            "name": "Bold",
            "id": "bold",
            "type": "boolean",
            "default": false
        },
        {
            "name": "Italic",
            "id": "italic",
            "type": "boolean",
            "default": false
        }
    ]
}
