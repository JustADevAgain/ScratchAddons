{
  "name": "Sticky footer",
  "description": "Makes the website footer stick out slightly from the bottom of the window and makes it expand when hovering over it. Previously part of \"Infinite scolling\".",
  "credits": [
    {
      "name": "Samq64",
      "link": "https://scratch.mit.edu/users/samq64"
    },
    {
      "name": "ZenithKnight"
    }
  ],
  "settings": [
    {
      "name": "Only on infinite scrolling pages",
      "id": "infiniteScroll",
      "type": "boolean",
      "if": {
        "addonEnabled": ["infinite-scroll"]
      },
      "default": false
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/*"]
    }
  ],
  "userstyles": [
    {
      "url": "userstyle.css",
      "matches": ["https://scratch.mit.edu/*"]
    }
  ],
  "tags": ["community"],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "versionAdded": "1.35.0"
}
