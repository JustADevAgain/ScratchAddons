{
  "name": "Thumbnails Setter",
  "description": "This addon will allow you to set the thubnail of your project to any image (including gif images)",
  "credits": [
    {
      "name": "World_Languages",
      "link": "https://github.com/WorldLanguages/animatedThumbnailsBookmarklet/blob/master/code.js"
    },
    {
      "name": "TheColaber",
      "link": "https://scratch.mit.edu"
    }
  ],
  "userscripts": [
    {
      "matches": ["https://scratch.mit.edu/projects/*"],
      "url": "userscript.js"
    }
  ],
  "userstyles": [
    {
      "url": "userstyle.css",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "tags": ["editor", "community"],
  "enabledByDefault": false
}
