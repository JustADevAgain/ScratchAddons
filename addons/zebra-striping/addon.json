{
  "$schema": "https://raw.githubusercontent.com/ScratchAddons/manifest-schema/dist/schema.json",
  "name": "Alternating nested block colors",
  "description": "Makes blocks of the same category alternate between lighter and darker shades when nested inside eachother.",
  "credits": [
    {
      "name": "CST1229",
      "link": "https://github.com/CST1229"
    }
  ],
  "tags": ["editor", "codeEditor"],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["projects"],
      "runAtComplete": true
    }
  ],
  "userstyles": [
    {
      "url": "userstyle.css",
      "matches": ["projects"]
    }
  ],
  "settings": [
    {
      "name": "Intensity (-100-100%)",
      "id": "intensity",
      "type": "integer",
      "min": -100,
      "default": 20,
      "max": 100
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "versionAdded": "1.28.0"
}
