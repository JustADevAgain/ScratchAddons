{
  "$schema": "https://raw.githubusercontent.com/ScratchAddons/manifest-schema/dist/schema.json",
  "name": "Zebra striping",
  "description": "Makes blocks of the same category alternate between lighter and darker colors when nested inside eachother.",
  "credits": [
    {
      "name": "CST1229",
      "link": "https://github.com/CST1229"
    }
  ],
  "tags": ["editor", "codeEditor", "theme"],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["projects"],
      "runAtComplete": false
    }
  ],
  "userstyles": [
    {
      "url": "userstyle.css",
      "matches": ["projects"]
    }
  ],
  "settings": [
    {
      "name": "Intensity (0-100%)",
      "id": "intensity",
      "type": "integer",
      "min": 0,
      "default": 10,
      "max": 100
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true
}
