{
  "name": "Fix unexpected script running",
  "description": "Fixes a bug in the Scratch editor that unexpectedly runs scripts when placing pasted or duplicated blocks inside of them.",
  "tags": ["editor", "codeEditor", "beta"],
  "credits": [
    {
      "name": "lisa_wolfgang",
      "link": "https://scratch.mit.edu/users/lisa_wolfgang"
    },
    {
      "name": "apple502j",
      "link": "https://github.com/apple502j"
    }
  ],
  "enabledByDefault": false,
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "info": [
    {
      "type": "notice",
      "text": "Reload the editor after enabling/disabling this addon.",
      "id": "reloadNotice"
    }
  ],
  "settings": [
    {
      "name": "Never run scripts when clicked",
      "id": "fullDisable",
      "type": "boolean",
      "default": false
    }
  ],
  "l10n": true
}
