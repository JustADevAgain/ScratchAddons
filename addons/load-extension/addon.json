{
  "name": "Auto-add extensions",
  "description": "Adds the pen extension automaticly.",
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "settings": [
    {
      "name": "Music",
      "id": "music",
      "type": "boolean",
      "default": true
    },
    {
      "name": "Pen",
      "id": "pen",
      "type": "boolean",
      "default": true
    },
    {
      "name": "Video Sensing",
      "id": "videoSensing",
      "type": "boolean",
      "default": true
    },
    {
      "name": "Text to Speach",
      "id": "text2speech",
      "type": "boolean",
      "default": true
    },
    {
      "name": "Translate",
      "id": "translate",
      "type": "boolean",
      "default": true
    }
  ],
  "tags": ["editor"],
  "l10n": true
}
