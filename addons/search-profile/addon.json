{
  "name": "Username search",
  "description": "Allows you to go to a user's profile when you search their username.",
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/search/*"],
      "runAtComplete": false
    }
  ],
  "userstyles": [
    {
      "url": "style.css",
      "matches": ["https://scratch.mit.edu/search/*"]
    }
  ],
  "settings": [
    {
      "name": "Show user icon",
      "id": "icon",
      "type": "boolean",
      "default": true
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "credits": [{ "name": "Geotale" }, { "name": "pufferfish101007" }, { "name": "Hexagonal" }, { "name": "valmontechno" }],
  "tags": ["community", "recommended", "profiles"],
  "versionAdded": "1.1.0",
  "latestUpdate": {
    "version": "1.33.2",
    "temporaryNotice": "Now displays the icon and name of the searched user.",
    "newSettings": ["icon"]
  },
  "enabledByDefault": false
}
