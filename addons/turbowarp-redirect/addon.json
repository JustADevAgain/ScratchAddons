{
  "name": "TurboWarp redirect",
  "description": "Redirects unshared projects to TurboWarp.",
  "tags": ["community"],
  "credits": [
    {
      "name": "illogicalapple",
      "link": "https://scratch.mit.edu/users/illogicalapple/"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "settings": [
  	{
      "name": "Redirect to",
      "id": "redirectUrl",
      "type": "select",
      "default": "//turbowarp.org/",
      "potentialValues": [
        {
          "id": "//turbowarp.org/",
          "name": "TurboWarp"
        },
        {
          "id": "//forkphorus.github.io/#",
          "name": "Forkphorus"
        }
      ]
    }
  ],
  "versionAdded": "1.26.0",
  "dynamicEnable": true,
  "enabledByDefault": true
}
