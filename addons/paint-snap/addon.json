{
  "$schema": "https://raw.githubusercontent.com/ScratchAddons/manifest-schema/dist/schema.json",
  "name": "Costume editor snapping",
  "description": "Snap objects in the costume editor to bounding boxes and vector nodes.",
  "tags": ["editor", "costumeEditor", "featured"],
  "versionAdded": "1.29.0",
  "userscripts": [
    {
      "matches": ["projects"],
      "url": "userscript.js",
      "runAtComplete": false
    }
  ],
  "userstyles": [
    {
      "matches": ["projects"],
      "url": "userstyle.css"
    }
  ],
  "settings": [
    {
      "type": "boolean",
      "id": "enable-default",
      "name": "Enable by default",
      "default": true
    },
    {
      "type": "color",
      "id": "guide-color",
      "name": "Snapping guide color",
      "default": "#ff0000"
    }
  ],
  "dynamicDisable": true,
  "dynamicEnable": true
}
