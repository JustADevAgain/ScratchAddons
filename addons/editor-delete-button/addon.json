{
  "$schema": "https://raw.githubusercontent.com/ScratchAddons/manifest-schema/dist/schema.json",
  "name": "Delete button in editor",
  "description": "Adds a delete button to the file menu that allows you to move your project to the trash folder.",
  "credits": [
    {
      "name": "Weredime (9gr)",
      "link": "https://scratch.mit.edu/users/9gr/"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["projects"]
    }
  ],
  "userstyles": [
    {
      "url": "userstyle.css",
      "matches": ["projects"]
    }
  ],
  "settings": [
    {
      "name": "Reset project before trashing",
      "type": "boolean",
      "default": false,
      "id": "resetProject"
    }
  ],
  "info": [
    {
      "id": "reset",
      "text": "If the setting \"Reset project before trashing\" is checked, the project will be reset before trashing it. You will not be able to undo this action.",
      "type": "warning"
    }
  ],
  "tags": ["editor", "editorMenuBar"],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "versionAdded": "1.28.0"
}
