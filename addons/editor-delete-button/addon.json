{
  "$schema": "https://raw.githubusercontent.com/ScratchAddons/manifest-schema/dist/schema.json",
  "name": "Delete button in editor",
  "description": "Adds a delete button to the file menu that allows you to delete your project.",
  "credits": [
    {
      "name": "Weredime (9gr)",
      "link": "https://scratch.mit.edu/users/9gr/"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["projects"]
    }
  ],
  "settings": [
    {
      "name": "Reset project before trashing",
      "type": "boolean",
      "default": true,
      "id": "resetProject"
    }
  ],
  "tags": ["editor", "editorMenuBar"],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "versionAdded": "1.28.0"
}
