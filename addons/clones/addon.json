    {
      "id": "showicononly",
      "name": "Show icon only",
      "default": false,
      "type": "boolean"
    }
  ],
  "latestUpdate": {
    "version": "1.30.0",
    "temporaryNotice": "You can now choose whether the clone counter shows up only in the editor or also on the project page."
  },
  "tags": ["editor", "projectPlayer", "recommended"],
