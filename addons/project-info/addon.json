{
  "name": "Sprite and script count",
  "description": "Shows the number of sprites and scripts a project has.",
  "tags": ["community"],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "userstyles": [
    {
      "url": "userscript.css",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "settings": [
    {
      "id": "editorCount",
      "name": "Live block count in editor",
      "type": "boolean",
      "default": false
    }
  ],
  "traps": true,
  "l10n": true
}
