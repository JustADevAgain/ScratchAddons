{
  "name": "Forums image uploader",
  "description": "Integrates image uploading into the forums.",
  "credits": [
    {
      "name": "Jeffalo"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": [
        "https://scratch.mit.edu/discuss/topic/*",
        "https://scratch.mit.edu/discuss/settings/*",
        "https://scratch.mit.edu/discuss/post/*/edit",
        "https://scratch.mit.edu/discuss/*/new"
      ]
    }
  ],
  "tags": ["forums", "community"],
  "enabledByDefault": false,
  "libraries": ["md5", "text-field-edit"],
  "l10n": true
}
