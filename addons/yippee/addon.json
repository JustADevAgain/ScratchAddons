{
  "name": "Eskuragarritasun doikuntzak",
  "description": "Editorearentzat eskuragarritasun bisual, kognitibo eta motorrak bermatzeko aldaketak.",
  "credits": [
    {
      "name": "SolomonkJr",
      "link": "https://scratch.mit.edu/users/SolomonkJr/"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["projects", "projectEmbeds"]
    }
  ],
  "info": [
    {
      "type": "info",
      "text": "Piktogramak kentzeko aukera itzali eta orria birkargatu.",
      "id": "pictoRestart"
    },
    {
      "type": "notice",
      "text": "\"Blokeak izkutatu\" atalean, F8 tekla sakatu izkutatu nahi duzun blokearen gainean. Blokea berreskuratzeko, birkargatu saioa.",
      "id": "f8Key"
    }
  ],
  "settings": [
    {
      "name": "Letra Mota",
      "id": "fontSelector",
      "type": "select",
      "default": "arial",
      "potentialValues": [
        {
          "id": "arial",
          "name": "Arial"
        },
        {
          "id": "verdana",
          "name": "Verdana"
        },
        {
          "id": "comicSans",
          "name": "Comic Sans"
        }
      ]
    },
    {
      "name": "Letra Tamaina",
      "id": "fontSizer",
      "type": "positive_integer",
      "default": 12,
      "min": 5,
      "max": 30
    },
    {
      "name": "@brush2.svg Piktogramak",
      "id": "texToPic",
      "type": "boolean",
      "default": false
    },
    {
      "name": "@list.svg Blokeak izkutatu (F8)",
      "id": "hideBlocks",
      "type": "boolean",
      "default": false
    },
    {
      "name": "@comment.svg Testua Ozen Irakurri",
      "id": "textToSpeechEnabled",
      "type": "boolean",
      "default": true
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "versionAdded": "1.33.0",
  "tags": ["editor", "codeEditor", "editorMenuBar"]
}
