{
  "name": "Featured project lists",
  "description": "Choose a list of featured projects/studios to be displayed on the front page",
  "credits": [
    {
      "name": "Norbiros",
      "link": "https://scratch.mit.edu/users/norbir/"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/"],
