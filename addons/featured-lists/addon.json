        { "name": "Featured Projects", "id": "FeaturedProjects" },
        { "name": "Featured Studios", "id": "FeaturedStudios" },
        { "name": "Projects Curated by", "id": "CuratedProjects" },
        { "name": "Scratch Studio", "id": "ScratchStudio" },
        { "name": "Projects Loved by", "id": "ProjectsLoved" },
        { "name": "What community is remixing", "id": "CommunityRemixing" },
        { "name": "What community is loving", "id": "CommunityLoving" }
      ],
      "row": [
        {
          "name": "Name",
          "id": "name",
          "type": "string",
          "default": ""
        },
        {
          "name": "Id",
          "id": "id",
          "type": "untranslated",
          "default": ""
        }
      ],
      "presets": [
        {
          "name": "Featured Projects",
          "values": { "name": "Featured Projects", "id": "FeaturedProjects" }
        },
        {
          "name": "Featured Studios",
          "values": { "name": "Featured Studios", "id": "FeaturedStudios" }
        },
        {
          "name": "Projects Curated by",
          "values": { "name": "Projects Curated by", "id": "CuratedProjects" }
        },
        {
          "name": "Scratch Studio",
          "values": { "name": "Scratch Studio", "id": "ScratchStudio" }
        },
        {
          "name": "Projects Loved by",
          "values": { "name": "Projects Loved by", "id": "ProjectsLoved" }
        },
        {
          "name": "What community is remixing",
          "values": { "name": "What community is remixing", "id": "CommunityRemixing" }
        },
        {
          "name": "What community is loving",
          "values": { "name": "What community is loving", "id": "CommunityLoving" }
        }
      ]
    }
  ],
  "versionAdded": "1.26.0",
  "tags": ["community", "featured"],
  "enabledByDefault": false
}
