{
  "name": "Featured project lists",
  "description": "Change order of lists that are displayed on the front page.",
  "credits": [
    {
      "name": "Norbiros",
      "link": "https://scratch.mit.edu/users/norbir/"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/"],
      "runAtComplete": true
    }
  ],
  "settings": [
    {
      "name": "Rows",
      "id": "rows",
      "type": "table",
      "disableEditing": true,
      "default": [
        { "name": "Featured Projects", "id": "community_featured_projects" },
        { "name": "Featured Studios", "id": "community_featured_studios" },
        { "name": "Projects Curated by", "id": "curator_top_projects" },
        { "name": "Scratch Design Studio", "id": "scratch_design_studio" },
        { "name": "Projects by Scratchers I'm Following", "id": "custom_projects_by_following" },
        { "name": "Projects Loved by Scratchers I'm Following", "id": "custom_projects_loved_by_following" },
        { "name": "Projects in Studios I'm Following", "id": "custom_projects_in_studios_following" },
        { "name": "What the Community is Remixing", "id": "community_most_remixed_project" },
        { "name": "What the Community is Loving", "id": "community_most_loved_projects" }
      ],
      "row": [
        {
          "name": "Name",
          "id": "name",
          "type": "string",
          "default": ""
        },
        {
          "name": "Id",
          "id": "id",
          "type": "untranslated",
          "default": ""
        }
      ],
      "presets": [
        {
          "name": "Featured Projects",
          "values": { "name": "Featured Projects", "id": "community_featured_projects" }
        },
        {
          "name": "Featured Studios",
          "values": { "name": "Featured Studios", "id": "community_featured_studios" }
        },
        {
          "name": "Projects Curated by",
          "values": { "name": "Projects Curated by", "id": "curator_top_projects" }
        },
        {
          "name": "Scratch Design Studio",
          "values": { "name": "Scratch Design Studio", "id": "scratch_design_studio" }
        },
        {
          "name": "Projects by Scratchers I'm Following",
          "values": { "name": "Projects by Scratchers I'm Following", "id": "custom_projects_by_following" }
        },
        {
          "name": "Projects Loved by Scratchers I'm Following",
          "values": { "name": "Projects Loved by Scratchers I'm Following", "id": "custom_projects_loved_by_following" }
        },
        {
          "name": "Projects in Studios I'm Following",
          "values": { "name": "Projects in Studios I'm Following", "id": "custom_projects_in_studios_following" }
        },
        {
          "name": "What the Community is Remixing",
          "values": { "name": "What the Community is Remixing", "id": "community_most_remixed_project" }
        },
        {
          "name": "What the Community is Loving",
          "values": { "name": "What the Community is Loving", "id": "community_most_loved_projects" }
        }
      ]
    }
  ],
  "dynamicDisable": true,
  "dynamicEnable": true,
  "versionAdded": "1.28.0",
  "tags": ["community", "featured"],
  "enabledByDefault": false
}
