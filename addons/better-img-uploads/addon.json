{
  "name": "HD image uploads",
  "description": "Automatically converts uploaded images into svg images to keep them from losing quality.",
  "tags": ["editor", "beta"],
  "credits": [
    {
      "name": "ErrorGamer2000",
      "link": "https://scratch.mit.edu/users/ErrorGamer2000/"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/projects/*"],
      "runAtComplete": true
    }
  ],
  "userstyles": [
    {
      "url": "style.css",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "settings": [
    {
      "name": "Image Fitting",
      "id": "fitting",
      "type": "select",
      "potentialValues": [
        {
          "id": "full",
          "name": "Full Size"
        },
        {
          "id": "fill",
          "name": "Fill Stage"
        },
        {
          "id": "fit",
          "name": "Fit to Stage"
        }
      ],
      "default": "fit"
    }
  ],
  "l10n": true
}
